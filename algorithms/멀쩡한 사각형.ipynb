   "outputs": [],
   "source": [
    "def gcd(a, b):\n",
    "    if a % b == 0:\n",
    "        return b\n",
    "    else:\n",
    "        return gcd(b, a % b)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7da08324-5e1c-4cd0-88f2-980b40fcdd6a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(w, h):\n",
    "    answer = w * h - (w + h - gcd(w, h))\n",
    "    return answer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "9bc3f071-aeb3-4fae-9d0b-a227775f3acb",
   "metadata": {},
   "outputs": [],
   "source": [
    "w, h = 8, 12\n",
    "assert 80 == solution(w, h)"
   ]
  },
